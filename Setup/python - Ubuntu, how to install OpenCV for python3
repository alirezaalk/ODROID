<div id="answer-37190408" class="answer accepted-answer" data-answerid="37190408" itemprop="acceptedAnswer" itemscope="" itemtype="http://schema.org/Answer">
    <div class="post-layout">
        <div class="votecell post-layout--left">
            <div class="js-voting-container grid fd-column ai-stretch gs4 fc-black-200" data-post-id="37190408">
